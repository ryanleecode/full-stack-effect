{
	"biome": {
		"indentStyle": "space",
		"quoteStyle": "single",
		"quoteProperties": "asNeeded",
		"semicolons": "always"
	},
	"json": {
		"useTabs": true
	},
	"markdown": {},
	"includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json}"],
	"excludes": [
		"**/node_modules",
		"build",
		"dist",
		"dist-dts",
		"dist.new",
		"**/tsup.config*.mjs"
	],
	"plugins": [
		"https://plugins.dprint.dev/json-0.19.2.wasm",
		"https://plugins.dprint.dev/markdown-0.15.2.wasm",
		"https://plugins.dprint.dev/biome-0.4.4.wasm"
	]
}
